cd openwrt && ./scripts/feeds install -a &&sudo rm -rf /feeds/luci/applications/luci-app-passwall/ &&sudo  rm -rf feeds/packages/lang/golang &&sudo rm -rf feeds/packages/net/{chinadns*,hysteria,geoview,trojan*,xray*,v2ray*,sing*} && sudo find ./ | grep Makefile | grep v2ray-geodata | xargs rm -f && find ./ | grep Makefile | grep mosdns | xargs rm -f  &&  sudo git clone https://github.com/sbwml/packages_lang_golang -b 24.x feeds/packages/lang/golang && sudo git clone https://github.com/sbwml/luci-app-mosdns -b v5 package/mosdns && sudo git clone https://github.com/sbwml/v2ray-geodata package/v2ray-geodata && echo 'src-git passwall2 https://github.com/xiaorouji/openwrt-passwall2.git' >>feeds.conf.default && echo 'src-git passwall_packages https://github.com/xiaorouji/openwrt-passwall-packages.git;main' >>feeds.conf.default && ./scripts/feeds update -a &&  ./scripts/feeds install -a
